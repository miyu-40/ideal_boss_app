<div class="container">
  <h1><%= t('.title') %></h1>
  <%= form_with url: password_resets_path, local: true, data: { turbo: false } do |f| %>
    <div>
      <%= f.label :email, t(User.human_attribute_name(:email)) %>
      <%= f.email_field :email, class: 'form-control' %>
    </div>
    <%= f.submit t('password_resets.new.submit'), class: 'btn btn-primary' %>
  <% end %>
</div>